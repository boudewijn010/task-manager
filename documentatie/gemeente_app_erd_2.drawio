<mxfile host="app.diagrams.net">
  <diagram id="uml-clean-types" name="UML Clean ERD">
    <mxGraphModel dx="1200" dy="1200" grid="1" gridSize="10">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>

        <!-- complaints -->
        <mxCell id="complaints" value="complaints" style="swimlane;fontStyle=1;startSize=32;horizontal=1;rounded=0;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="40" y="40" width="310" height="350" as="geometry"/>
        </mxCell>
        <mxCell value="+ id : INT (PK)&#10;+ title : VARCHAR&#10;+ description : TEXT&#10;+ type : ENUM&#10;+ status : ENUM&#10;+ priority : ENUM&#10;+ contact_email : VARCHAR&#10;+ address : VARCHAR&#10;+ latitude : DECIMAL&#10;+ longitude : DECIMAL&#10;+ response : TEXT&#10;+ created_at : DATETIME&#10;+ updated_at : DATETIME&#10;+ photo_path : VARCHAR" 
          style="text;align=left;verticalAlign=top;whiteSpace=wrap;overflow=hidden;fontSize=12;" vertex="1" parent="complaints">
          <mxGeometry x="0" y="32" width="310" height="318" as="geometry"/>
        </mxCell>

        <!-- complaint_comments -->
        <mxCell id="complaint_comments" value="complaint_comments" style="swimlane;fontStyle=1;startSize=32;horizontal=1;rounded=0;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="380" y="40" width="270" height="200" as="geometry"/>
        </mxCell>
        <mxCell value="+ id : INT (PK)&#10;+ complaint_id : INT (FK)&#10;+ user_email : VARCHAR&#10;+ comment : TEXT&#10;+ created_at : DATETIME"
          style="text;align=left;verticalAlign=top;whiteSpace=wrap;overflow=hidden;fontSize=12;" vertex="1" parent="complaint_comments">
          <mxGeometry x="0" y="32" width="270" height="168" as="geometry"/>
        </mxCell>

        <!-- complaint_notes -->
        <mxCell id="complaint_notes" value="complaint_notes" style="swimlane;fontStyle=1;startSize=32;horizontal=1;rounded=0;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="380" y="270" width="270" height="200" as="geometry"/>
        </mxCell>
        <mxCell value="+ id : INT (PK)&#10;+ complaint_id : INT (FK)&#10;+ user_email : VARCHAR&#10;+ note : TEXT&#10;+ created_at : DATETIME"
          style="text;align=left;verticalAlign=top;whiteSpace=wrap;overflow=hidden;fontSize=12;" vertex="1" parent="complaint_notes">
          <mxGeometry x="0" y="32" width="270" height="168" as="geometry"/>
        </mxCell>

        <!-- gebruikers -->
        <mxCell id="gebruikers" value="gebruikers" style="swimlane;fontStyle=1;startSize=32;horizontal=1;rounded=0;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="40" y="420" width="270" height="180" as="geometry"/>
        </mxCell>
        <mxCell value="+ ID : INT (PK)&#10;+ name : VARCHAR&#10;+ password : VARCHAR&#10;+ completed : INT"
          style="text;align=left;verticalAlign=top;whiteSpace=wrap;overflow=hidden;fontSize=12;" vertex="1" parent="gebruikers">
          <mxGeometry x="0" y="32" width="270" height="148" as="geometry"/>
        </mxCell>

        <!-- tasks -->
        <mxCell id="tasks" value="tasks" style="swimlane;fontStyle=1;startSize=32;horizontal=1;rounded=0;strokeColor=#000000;fillColor=#FFFFFF;" vertex="1" parent="1">
          <mxGeometry x="380" y="500" width="300" height="250" as="geometry"/>
        </mxCell>
        <mxCell value="+ id : BIGINT (PK)&#10;+ title : VARCHAR&#10;+ description : TEXT&#10;+ status : ENUM&#10;+ priority : ENUM&#10;+ due_date : DATETIME&#10;+ created_at : DATETIME&#10;+ updated_at : DATETIME"
          style="text;align=left;verticalAlign=top;whiteSpace=wrap;overflow=hidden;fontSize=12;" vertex="1" parent="tasks">
          <mxGeometry x="0" y="32" width="300" height="218" as="geometry"/>
        </mxCell>

        <!-- Relations -->
        <mxCell edge="1" parent="1" source="complaints" target="complaint_comments">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>
        <mxCell edge="1" parent="1" source="complaints" target="complaint_notes">
          <mxGeometry relative="1" as="geometry"/>
        </mxCell>

      </root>
    </mxGraphModel>
  </diagram>
</mxfile>